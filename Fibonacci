pipeline {
    agent any
    
    stages {
        stage('Build Docker image') {
            steps {
                script {
                    docker.build('fibonacci-image')
            }
        }
}        
        stage('Push') {
            steps {
                // Push the Docker image to Docker Hub
                    docker.withRegistry('https://registry.hub.docker.com', 'Docker_Login') 
            }
        }
        stage('Cleanup') { steps { cleanWs() } }
}
}
